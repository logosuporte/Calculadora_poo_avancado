unit Model.Soma;

interface
uses Model.Calculadora.interfaces,System.SysUtils, Model.Calculadora.Operacoes;
type
  TSoma = Class(TOperacoes)
  public
    Constructor Create;
    Destructor  Destroy; override;
    class function New: iOperacoes;
    function Executar:string; override;
  End;

implementation

{ TSoma }

constructor TSoma.Create;
begin

end;

destructor TSoma.Destroy;
begin

  inherited;
end;

function TSoma.Executar: string;
begin
  inherited;

end;

class function TSoma.New: iOperacoes;
begin
  Result := Self.Create;
end;




end.
